<Window x:Class="SWD.LeaveOneOutWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SWD"
        xmlns:oxy="http://oxyplot.codeplex.com"
        xmlns:oxy1="http://oxyplot.org/wpf"
        mc:Ignorable="d"
        Title="LeaveOneOutWindow" Height="450" Width="800">
    <Grid>
        <ComboBox x:Name="metricsComboBox" HorizontalAlignment="Left" Margin="20,23,0,0" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="classifyButton" Content="Klasyfikuj" HorizontalAlignment="Left" Margin="175,23,0,0" VerticalAlignment="Top" Width="75" Click="ClassifyClick"/>
        <oxy1:PlotView Model="{Binding MyModel}" Name="plot" Margin="10,55,10,10"/>
    </Grid>
</Window>
