<Window x:Class="SWD.EDMoreDimentions"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SWD"
        mc:Ignorable="d"
        Title="EDMoreDimentions" Height="250" Width="600">
    <Grid>
        <Button x:Name="classifyAll" Content="Klasyfikuj" HorizontalAlignment="Left" Margin="50,42,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="0.333,-0.5" Click="ClassifyAll_Click"/>
        <Label x:Name="label" Content="Klasyfikuj wszystkie" HorizontalAlignment="Left" Margin="45,11,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="30" Margin="50,105,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="385"/>
        <Label x:Name="label1" Content="Klasyfikacja jednego obiektu(wartości oddzielane przecinkiem)" HorizontalAlignment="Left" Margin="50,79,0,0" VerticalAlignment="Top"/>
        <Button x:Name="button" Content="Klasyfikuj" HorizontalAlignment="Left" Margin="455,115,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click" IsEnabled="False"/>

    </Grid>
</Window>
