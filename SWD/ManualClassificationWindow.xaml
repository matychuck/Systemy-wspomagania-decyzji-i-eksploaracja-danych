        xmlns:local="clr-namespace:SWD"
        mc:Ignorable="d"
        Title="ManualClassificationWindow" Height="150" Width="300">
    <Grid>
        <Button x:Name="button" Content="Ok" HorizontalAlignment="Left" Margin="170,63,0,0" VerticalAlignment="Top" Width="75" Click="Ok_Click"/>
        <Label x:Name="classLabel" Content="Label" HorizontalAlignment="Left" Margin="20,34,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="25,60,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="stepLabel" Content="1/10" HorizontalAlignment="Left" Margin="124,79,0,0" VerticalAlignment="Top" FontSize="10"/>

    </Grid>
