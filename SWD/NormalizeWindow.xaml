<Window x:Class="SWD.NormalizeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SWD"
        mc:Ignorable="d"
        Title="Normalizacja zmiennych" Height="200" Width="500">
    <Grid>
        <ComboBox x:Name="comboBox" HorizontalAlignment="Left" Margin="30,63,0,0" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label" Content="Wybierz kolumnę" HorizontalAlignment="Left" Margin="25,37,0,0" VerticalAlignment="Top"/>
        <Button x:Name="button" Content="OK" HorizontalAlignment="Left" Margin="135,135,0,0" VerticalAlignment="Top" Width="75" Click="NormalizeButtonOk_Click"/>
        <Button x:Name="button1" Content="Cancel" HorizontalAlignment="Left" Margin="230,135,0,0" VerticalAlignment="Top" Width="75" Click="NormalizeButtonCancel_Click"/>
        <Button x:Name="button2" Content="Normalize All" HorizontalAlignment="Left" Margin="325,135,0,0" VerticalAlignment="Top" Width="75" Click="NormalizeAllButton_Click"/>
    </Grid>
</Window>
